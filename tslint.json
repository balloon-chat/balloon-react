{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "max-line-length": [
      true,
      120
    ],
    "object-shorthand-properties-first": false,
    "radix": false,
    "ter-arrow-parens": false,
    "import-name": [
      true,
      {
        "react": "React",
        "styledComponents": "styled",
        "image": "Image",
        "head": "Head",
        "reactImagesUploading": "ReactImageUploading",
        "app": "firebase",
        "link": "Link",
        "reactInfiniteScrollComponent": "InfiniteScroll",
        "reactCopyToClipboard": "CopyToClipboard",
        "matterJs": "Matter",
        "p5": "P5Types"
      }
    ]
  }
}
